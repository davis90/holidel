{
    "name": "holidel-api",
    "version": "0.1.0",
    "private": true,
    "main": "dist/index.js",
    "scripts": {
        "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
        "build": "tsc -p tsconfig.json",
        "start": "node dist/index.js",
        "lint": "echo 'Add linting command'",
        "typeorm:migrate": "ts-node src/scripts/run-migrations.ts",
        "typeorm:revert": "ts-node src/scripts/revert-migrations.ts",
        "typeorm:seed:test": "ts-node src/scripts/seed-test-data.ts",
        "test:clean:test": "ts-node src/scripts/clean-test-data.ts",
        "test:api:filters": "ts-node src/scripts/test-offers-filters.ts",
        "test:seed:data": "ts-node src/scripts/seed-nurse-with-offers.ts",
        "db:init": "npm run typeorm:migrate"
    },
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "dotenv": "^16.0.0",
        "express": "^4.18.2",
        "express-validator": "^7.3.1",
        "helmet": "^6.0.0",
        "jsonwebtoken": "^9.0.2",
        "libphonenumber-js": "^1.10.54",
        "morgan": "^1.10.0",
        "pg": "^8.11.0",
        "reflect-metadata": "^0.1.13",
        "typeorm": "^0.3.17"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.2",
        "@types/express": "^4.17.17",
        "@types/jsonwebtoken": "^9.0.2",
        "@types/morgan": "^1.9.4",
        "@types/node": "^18.16.2",
        "ts-node": "^10.9.1",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.1.6"
    }
}
